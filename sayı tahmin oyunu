import random


sayı = random.randint(1,50)

hak= 5 

while hak>0 : 
    hak -= 1
    tahmin=int(input("tahmin:"))

    if sayı== tahmin:
        print(f"tebrikler tuttuğum sayı {sayı} ")
        break

    elif sayı>tahmin:
        print("yukarı")
    else:
            print("aşağı")


    
if hak==0:
         print(f"hakkınız bitti , tutulan sayı {sayı}")

print(sayı)
